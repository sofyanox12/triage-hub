        "pino": "^9.14.0",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/pg": "^8.11.11",
        "eslint": "^9.39.2",
        "eslint-plugin-import-x": "^4.16.1",
        "globals": "^17.3.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.55.0"
    }
}
{
    "name": "@triage/shared",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./server": {
            "types": "./dist/server.d.ts",
            "default": "./dist/server.js"

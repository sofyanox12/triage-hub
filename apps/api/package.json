{
    "name": "@triage/api",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=20",
        "pnpm": ">=9"
    },
    "main": "dist/server.js",
    "types": "dist/server.d.ts",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "tsx src/server.ts",
        "lint": "eslint src",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\"",
        "type-check": "tsc --noEmit",
        "test:non-blocking": "tsx src/scripts/non-blocking-check.ts",
        "db:migrate": "kysely migrate:latest",
        "db:rollback": "kysely migrate:rollback",
        "db:status": "kysely migrate:status",
        "db:make": "kysely migrate:make",
        "db:reset:tickets": "tsx src/scripts/db-reset.ts --tickets",
        "db:reset:users": "tsx src/scripts/db-reset.ts --users",
        "db:reset:all": "tsx src/scripts/db-reset.ts --all"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@triage/shared": "workspace:*",
        "bcryptjs": "^3.0.3",
        "bullmq": "^5.36.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^5.2.1",
        "ioredis": "^5.9.3",
        "jsonwebtoken": "^9.0.3",
        "kysely": "^0.28.11",
        "openai": "^4.104.0",
        "pino": "^9.6.0",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/bcryptjs": "^3.0.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.10",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-unused-imports": "^4.4.1",
        "globals": "^17.3.0",
        "kysely-ctl": "^0.20.0",
        "prettier": "^3.8.1",
        "tsc-alias": "^1.8.16",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.55.0"
    }
}